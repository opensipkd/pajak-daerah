	<queryString>
		<![CDATA[SELECT  k.kd_propinsi||'.'||k.kd_dati2||'-'||k.kd_kecamatan kode, k.nm_kecamatan uraian, dt2.nm_dati2, 'Semua Kecamatan' as nm_kecamatan, 'Semua Kelurahan' as nm_kelurahan, count(s.kd_propinsi||s.kd_dati2||s.kd_kecamatan) sppt1, sum(coalesce(amount1,0)) amount1,  sum(coalesce(amount2,0)) amount2, sum(coalesce(amount1,0)) - sum(coalesce(amount2,0)) amount3
FROM ref_kecamatan k
LEFT JOIN (
SELECT a.kd_propinsi, a.kd_dati2, a.kd_kecamatan, a.kd_kelurahan, a.kd_blok , a.no_urut, a.kd_jns_op, a.nm_wp_sppt uraian, 1 sppt1, a.pbb_yg_harus_dibayar_sppt as amount1, sum(jml_sppt_yg_dibayar-denda_sppt) as amount2
FROM sppt a
INNER JOIN pembayaran_sppt b ON  a.kd_propinsi = b.kd_propinsi AND a.kd_dati2 = b.kd_dati2 AND a.kd_kecamatan = b.kd_kecamatan AND a.kd_kelurahan = b.kd_kelurahan AND a.kd_blok = b.kd_blok  AND a.no_urut = b.no_urut  AND a.kd_jns_op = b.kd_jns_op  AND a.thn_pajak_sppt = b.thn_pajak_sppt
WHERE a.thn_pajak_sppt='$P!{tahun}'
AND a.kd_propinsi='$P!{kd_propinsi}'
AND a.kd_dati2='$P!{kd_dati2}'
GROUP BY a.kd_propinsi, a.kd_dati2, a.kd_kecamatan, a.kd_kelurahan, a.kd_blok, a.no_urut, a.kd_jns_op, a.thn_pajak_sppt
HAVING a.pbb_yg_harus_dibayar_sppt<sum(jml_sppt_yg_dibayar-denda_sppt)
) s
ON k.kd_propinsi = s.kd_propinsi
AND k.kd_dati2 = s.kd_dati2
AND k.kd_kecamatan = s.kd_kecamatan
INNER JOIN ref_dati2 dt2 ON k.kd_propinsi = dt2.kd_propinsi
AND k.kd_dati2 = dt2.kd_dati2
WHERE (1=1) AND (1=1)
GROUP BY k.kd_propinsi, k.kd_dati2, k.kd_kecamatan, k.nm_kecamatan, dt2.nm_dati2
ORDER BY k.kd_propinsi, k.kd_dati2, k.kd_kecamatan]]>
	</queryString>
