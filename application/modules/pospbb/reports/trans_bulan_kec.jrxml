	<queryString>
		<![CDATA[SELECT  Extract(month FROM tgl_pembayaran_sppt) kode,
          $P!{pos_uraian}||':'||tp.nm_tp uraian, p.thn_pajak_sppt, dt2.nm_dati2, kec.nm_kecamatan, 'Semua Kelurahan' AS nm_kelurahan, sum(Coalesce(p.jml_sppt_yg_dibayar,0)-Coalesce(p.denda_sppt,0)) AS pokok,
          sum(Coalesce(p.denda_sppt,0)) AS denda, sum(Coalesce(p.jml_sppt_yg_dibayar,0)) AS bayar
FROM sppt k
INNER JOIN ref_dati2 dt2 ON k.kd_propinsi=dt2.kd_propinsi AND k.kd_dati2=dt2.kd_dati2
INNER JOIN ref_kecamatan kec ON kec.kd_propinsi=k.kd_propinsi AND kec.kd_dati2=k.kd_dati2 AND kec.kd_kecamatan=k.kd_kecamatan
INNER JOIN pembayaran_sppt p ON k.kd_propinsi = p.kd_propinsi AND k.kd_dati2 = p.kd_dati2 AND k.kd_kecamatan = p.kd_kecamatan AND k.kd_kelurahan = p.kd_kelurahan AND k.kd_blok = p.kd_blok AND k.no_urut = p.no_urut AND k.kd_jns_op = p.kd_jns_op AND k.thn_pajak_sppt = p.thn_pajak_sppt
LEFT JOIN tempat_pembayaran tp ON $P!{pos_join}
WHERE k.kd_propinsi='$P!{kd_propinsi}' AND k.kd_dati2='$P!{kd_dati2}' AND k.kd_kecamatan='$P!{kd_kecamatan}' AND
extract(year FROM p.tgl_pembayaran_sppt)=$P!{tahun} AND p.thn_pajak_sppt between '$P!{tahun_sppt1}' AND '$P!{tahun_sppt2}' AND k.pbb_yg_harus_dibayar_sppt between $P!{bukumin} AND $P!{bukumax} $P!{where_tp}
GROUP BY 1,2,3,4,5,6
ORDER BY 1,2,3]]>
	</queryString>
